{
  "rewrites": [
    {
      "source": "/sign-up/sso-callback",
      "destination": "/sign-up"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/sign-in",
      "destination": "/sign-in",
      "permanent": false
    },
    {
      "source": "/sign-up",
      "destination": "/sign-up",
      "permanent": false
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/sign-up/sso-callback",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
